{
	"name": "six-cities",
	"version": "1.0.0",
	"private": true,
	"description": "Личный проект «Шесть городов» от HTML Academy",
	"scripts": {
		"eslint": "eslint --ext .tsx --ext .ts src/",
		"test": "eslint --ext .tsx --ext .ts src/ && yarn test.jest",
		"build": "webpack --mode production",
		"start": "webpack-dev-server",
		"test.jest": "jest"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>/src/test/style-mock.ts"
		},
		"setupFilesAfterEnv": [
			"./src/test/setup-tests.ts"
		]
	},
	"devDependencies": {
		"@babel/core": "7.12",
		"@babel/plugin-proposal-class-properties": "7.13.0",
		"@babel/plugin-proposal-object-rest-spread": "7.13.8",
		"@babel/plugin-proposal-optional-chaining": "7.13.12",
		"@babel/preset-env": "7.12",
		"@babel/preset-react": "7.12",
		"@babel/preset-typescript": "7.13.0",
		"@testing-library/jest-dom": "5.11.10",
		"@testing-library/react": "11.2",
		"@types/jest": "26.0.22",
		"@typescript-eslint/eslint-plugin": "4.20.0",
		"@typescript-eslint/parser": "4.20.0",
		"axios-mock-adapter": "1.19",
		"babel-loader": "8.2",
		"css-loader": "5.0",
		"dotenv-webpack": "6.0.4",
		"eslint": "7.23.0",
		"eslint-config-htmlacademy": "2.1.0",
		"eslint-plugin-react": "7.23.1",
		"file-loader": "6.2",
		"jest": "26.6",
		"redux-devtools-extension": "2.13",
		"redux-mock-store": "1.5.4",
		"style-loader": "2.0",
		"ts-loader": "8.1.0",
		"typescript": "4.2.3",
		"webpack": "4.46",
		"webpack-cli": "3.3",
		"webpack-dev-server": "3.11"
	},
	"engines": {
		"node": ">=12.0.0"
	},
	"dependencies": {
		"@hookform/resolvers": "1.3.5",
		"@types/leaflet": "1.7.0",
		"@types/react": "16.14",
		"@types/react-dom": "17.0.2",
		"@types/react-redux": "7.1.16",
		"@types/react-router-dom": "5.1.7",
